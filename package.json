{
  "name": "react_and_express",
  "version": "1.0.0",
  "description": "Boilerplate for React and Express",
  "main": "server.js",
  "scripts": {
    "client-install": "cd client && npm install",
    "start": "cd server && node starter.js",
    "server": "cd server && nodemon starter.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "author": "Rick Warling",
  "license": "MIT",
  "dependencies": {
    "concurrently": "^4.0.1",
    "crypto-js": "^3.1.9-1",
    "express": "^4.16.4",
    "level": "^4.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.1.6",
    "@babel/preset-env": "^7.1.6",
    "@babel/preset-es2017": "^7.0.0-beta.53",
    "@babel/register": "^7.0.0"
  }
}
